<%- include('../header.ejs') %>

<section class="dashboard-wrap mtb-40">
  <div class="container">
    <div class="body-content">
      <div class="row">
        <div class="col-md-3" style="box-shadow: 2px 2px 8px green">
          <div class="dash-left">
            <ul>
              <li class="active"><a href="/admin/dashboard">Dashboard</a></li>
              <li><a href="/admin/blog" class="active">Blogs</a></li>
              <li><a href="/admin/category">Categories</a></li>
              <li><a href="/admin/contact">Contact</a></li>
              <li><a href="/admin/about">About</a></li>
              <li><a href="/logout">Logout</a></li>
            </ul>
          </div>
        </div>
        <div class="col-md-9">
          <div class="dash-right">
            <div class="dash-header">
              <div class="dash-title">
                <h1>About</h1>
              </div>
            </div>
            <table class="table table-bordered table-striped">
              <tr>
                <th>description</th>

                <!-- <th>Action</th> -->
              </tr>

              <% d.forEach((b)=>{ %>

              <tr>
                <td><%= b.description %></td>
                </tr>
                <td>
                  <a href="/admin/aboutedit/<%= b._id %>" class="btn btn-danger">edit</a>
                </td>
              <!-- </tr> -->

              <% }) %>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Modal -->
</section>

<%- include('../footer.ejs') %>
